    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        int n1 = Integer.parseInt(jTextField1.getText());
        float n2 = Float.parseFloat(jTextField2.getText());
        float imc = n1 / (n2 * n2);
        jTextField3.setText("Seu IMC Ã© " + imc);
    }                                        

    
